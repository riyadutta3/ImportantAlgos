#include <bits/stdc++.h>
using namespace std;

void max_heap(int *arr,int n)
{
    int i=n;
    while(i>1)
    {
        int parent=i/2;
        if(arr[parent]>arr[i])
        {
            swap(arr[parent],arr[i]);
            i=parent;
        }
        else
        return;
    }
    
    return;
}

int main()
{
    int n;
    cin>>n;
    int arr[n+1];
    n++;
    arr[0]=0;
    
    for(int i=1;i<=n;i++)
    {
        cin>>arr[i];
    }
    
    int value;
    cin>>value;
    arr[n+1]=value;
    
    max_heap(arr,n);

    for(int i=1;i<n+1;i++)
    {
        cout<<arr[i];
    }

    return 0;
}
